<%@ Page Title="" Language="C#" MasterPageFile="~/Site.Master" AutoEventWireup="true" CodeBehind="Programas.aspx.cs" Inherits="iColprov.Web.Forms.Programas" %>

<asp:Content ID="Content1" ContentPlaceHolderID="MainContent" runat="server">

    <div class="formulario container">

        <div class="row mt-4">

            <div class="col">

                <h2 style="text-align: center;" class="title">Crear programa</h2>

            </div>
        </div>
        <div class="row g-2 mt-4 mx-4">

            <div class="row g-2">

                <div class="col-md-4">
                    <label for="txtCodigo" class="form-label">Código</label>
                    <input type="text" class="form-control" id="txtCodigo" placeholder="" required>
                </div>

                <div class="col-md-4">
                    <label for="txtName" class="form-label">Nombre</label>
                    <input type="text" class="form-control" id="txtName" placeholder="" required>
                </div>

                <div class="col-md-4">
                    <label for="txtAbreviatura" class="form-label">Abreviatura</label>
                    <input type="text" class="form-control" id="txtAbreviatura" placeholder="" required>
                </div>
            </div>

            <div class="row g-2">

                <div class="col-md-4">
                    <label for="txtNoResolucion" class="form-label">N° Res. Autorización</label>
                    <input type="text" class="form-control" id="txtNoResolucion" placeholder="Nro. de resolucion de Autorización" required>
                </div>

                <div class="col-md-4">
                    <label for="txtFechaResolucion" class="form-label">Fecha Res. Autorización</label>
                    <input type="date" class="form-control" id="txtFechaResolucion" placeholder="Fecha de Resolucion de Autorización" required>
                </div>

                <div class="col-md-4">
                    <label class="control-label" for="txtAplicaPreins">Aplica para preinscripciones</label>

                    <div class="check-control sm">

                        <label class="RadioSi" for="txtRadioSi1">
                            <input type="radio" checked="checked" id="txtRadioSi1" name="txtAplicaPreins" value="true">
                            Si
                        </label>

                        <label class="RadioNo">
                            <input type="radio" id="txtRadio1" name="txtAplicaPreins" value="false">
                            No
                        </label>
                    </div>
                </div>
            </div>

            <div class="row g-2">

                <div class="col-md-4">
                    <label class="control-label" for="txtAplicaGrupos">Aplica para grupos</label>

                    <div class="check-control sm">
                        <label class="radioSi" for="txtRadioSi2">
                            <input type="radio" checked="checked" id="txtRadioSi2" name="AplicaGrupos" value="true">
                            Si
                        </label>

                        <label class="radioNo" for="txtRadioNo2">
                            <input type="radio" id="txtRadioNo2" name="AplicaGrupos" value="false">
                            No
                        </label>
                    </div>
                </div>

                <div class="col-md-4">
                    <label for="inputTipoEvaluacion" class="form-label">Tipo de evaluación</label>
                    <input type="text" class="form-control" id="inputTipoEvaluacion" placeholder="" required>
                </div>

                <div class="col-md-4">
                    <label class="control-label" for="txtEstado">Estado</label>

                    <div class="check-control sm">
                        <label class="radioActivo" for="txtRadioActivo">
                            <input type="radio" checked="checked" id="txtRadioActivo" name="Estado" value="true">
                            Activo
                        </label>

                        <label class="RadioInactivo" for="txtRadioInactivo">
                            <input type="radio" id="txtRadioInactivo" name="Estado" value="false">
                            Inactivo
                        </label>
                    </div>
                </div>
            </div>

            <div class="row g-3 m">
                <div class="d-grid gap-2 d-md-flex justify-content-md-end">
                    <button type="reset" class="btn btn-secondary">Cancelar</button>
                    <button type="submit" class="btn btn-primary">Aceptar</button>
                </div>
            </div>
        </div>
    </div>
